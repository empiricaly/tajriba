{
  "name": "@empirica/tajriba",
  "version": "1.0.0-alpha.2",
  "description": "Tajriba client",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empiricaly/tajriba.git"
  },
  "bugs": {
    "url": "https://github.com/empiricaly/tajriba/issues"
  },
  "homepage": "https://github.com/empiricaly/tajriba#readme",
  "keywords": [
    "tajriba",
    "experiment",
    "research",
    "multiplayer",
    "real-time",
    "behavioral",
    "social-science",
    "psychology"
  ],
  "author": "Nicolas Paton <nicolas.paton@gmail.com>",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "volta": {
    "node": "16.15.0",
    "npm": "8.10.0"
  },
  "scripts": {
    "pkg": "parcel build",
    "prebuild": "npm run generate",
    "build": "npm run pkg",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen -w",
    "prerelease": "npm run build",
    "release:next": "np --tag next",
    "release": "np --tag latest",
    "postversion": "npm version --no-git-tag-version --no-commit-hooks $npm_package_version",
    "test": "ava"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typed-document-node": "2.2.11",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-urql": "3.5.10",
    "@graphql-typed-document-node/core": "3.1.1",
    "@parcel/packager-ts": "2.5.0",
    "@parcel/transformer-typescript-types": "2.5.0",
    "ava": "4.2.0",
    "graphql-codegen-typescript-validation-schema": "0.5.0",
    "np": "7.5.0",
    "parcel": "2.5.0",
    "pika-plugin-package.json": "1.0.2",
    "prettier": "2.2.1",
    "tiny-typed-emitter": "2.1.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "graphql": "15.8.0",
    "graphql-tag": "2.12.6",
    "graphql-ws": "5.8.2",
    "isomorphic-ws": "4.0.1",
    "urql": "2.2.0",
    "websocket": "1.0.34",
    "wonka": "4.0.15",
    "zod": "3.16.0"
  }
}
